{
  "name": "stackgazer",
  "version": "1.0.0",
  "type": "module",
  "description": "A single-page app for viewing Go stack traces",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "python3 -m http.server 8000",
    "build:bundle": "npm run build && node scripts/bundle.cjs",
    "test": "node --test tests/**/*.test.js"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}